<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/start"
        motion:constraintSetStart="@id/end"
        motion:duration="300">

        <KeyFrameSet>

            <KeyAttribute
                motion:motionTarget="@+id/imageView4"
                motion:framePosition="40"
                android:scaleX="1.1"
                android:scaleY="1.1" />

            <KeyAttribute
                motion:motionTarget="@+id/imageView4"
                motion:framePosition="100"
                android:scaleX="1"
                android:scaleY="1" />


        </KeyFrameSet>

        <OnClick motion:targetId="@+id/imageView4" />

    </Transition>


    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="300">

        <KeyFrameSet>

            <KeyAttribute
                motion:motionTarget="@+id/imageView6"
                motion:framePosition="40"
                android:rotationY="90"
                 />

        </KeyFrameSet>

        <OnClick motion:targetId="@+id/imageView6" />

    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/start2">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end2">
    </ConstraintSet>
</MotionScene>