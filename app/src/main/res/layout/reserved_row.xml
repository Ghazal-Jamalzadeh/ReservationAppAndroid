<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    >

    <RelativeLayout
        android:id="@+id/relative_layout_click"
        android:elevation="4dp"

        android:layout_width="@dimen/_60cdp"
        android:layout_height="@dimen/_60cdp"
        android:background="@drawable/row_transparent_reserve"
        android:layout_margin="@dimen/_10cdp"

        >

        <TextView
            android:id="@+id/time_reserve"
            style="@style/fontForMediumEditText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_centerInParent="true"
            android:text="14:00" />

    </RelativeLayout>





</RelativeLayout>